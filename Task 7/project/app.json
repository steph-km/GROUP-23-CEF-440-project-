{
  "expo": {
    "name": "Trackify",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/track.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/track.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser","expo-task-manager", "expo-background-fetch"],
    "android": {
    "permissions": ["ACCESS_BACKGROUND_LOCATION", "ACCESS_FINE_LOCATION"]
  },
    "experiments": {
      "typedRoutes": true
    }
  }
}
